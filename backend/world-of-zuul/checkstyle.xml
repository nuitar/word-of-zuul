        <!-- 禁用 JavadocMethod 规则 -->
        <module name="JavadocMethod">
            <property name="scope" value="private"/>
        </module>
        <module name="JavadocMethod">
            <property name="scope" value="public"/>
        </module>
        <!-- 其他规则配置 -->
    </module>
</module>
